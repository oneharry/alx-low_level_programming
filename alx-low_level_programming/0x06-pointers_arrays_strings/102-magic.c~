#include <stdio.h>

int main(void)
{
	int z = 2;
	int y = 1;
	int n;
	int a[4];
	int *p;
	int x = 0;
	a[2] = 1024;
	p = &n;
	/*
	 * write your line of code here...
	 * Remember:
	 * - you are not allowed to use a
	 * - you are not allowed to modify p
	 * - only one statement
	 * - you are not allowed to code anything else than this line of code
	 */
	*(p + 7) = 100;
	printf("a[2] = %d\n", a[2]);
	printf("%d%d%d\n", z,x,y);
	return (0);
}
